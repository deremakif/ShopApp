@model ShopApp.WebUI.Models.ProductListModel


<div class="row">
    <div class="col-md-3">
        <ul class="list-group">
            <li class="list-group-item">
                Telefon
            </li>
            <li class="list-group-item">
                Bilgisayar
            </li>
            <li class="list-group-item">
                Elektronik
            </li>
        </ul>
    </div>
    <div class="col-md-9">
        <partial name="_slider" />

        @foreach (var item in Model.Products)
        {
            <p>@item.Name</p>
        }
    </div>

</div>

